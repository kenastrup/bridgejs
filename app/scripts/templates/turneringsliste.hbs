<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">Turneringer</h3>
    </div>
    <div class="panel-body">
        <table class="table table-bordered table-striped table-hover">
            <thead>
            <tr>
                <th>Navn</th>
                <th>Dato</th>
                <th>Antall spill</th>
                <th>Antall par</th>
            </tr>
            </thead>
            <tbody>
                {{#each turneringer}}
                <tr>
                    <td>{{navn}}</td>
                    <td>{{dato}}</td>
                    <td>{{antall_spill}}</td>
                    <td>{{antall_par}}</td>
                </tr>
                {{/each}}

            </tbody>

        </table>
    </div>
</div>
